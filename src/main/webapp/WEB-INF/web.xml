<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>WeatherPlanning</display-name>
  <servlet>
	<servlet-name>VacationServlet</servlet-name>
	<servlet-class>csci310.VacationServlet</servlet-class>
  </servlet>
   <servlet>
	<servlet-name>LandingServlet</servlet-name>
	<servlet-class>csci310.LandingServlet</servlet-class>
  </servlet>
     <servlet>
	<servlet-name>ActivityServlet</servlet-name>
	<servlet-class>csci310.ActivityServlet</servlet-class>
  </servlet>
     <servlet>
	<servlet-name>AnalysisServlet</servlet-name>
	<servlet-class>csci310.AnalysisServlet</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>LoginServlet</servlet-name>
	<servlet-class>csci310.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>VacationServlet</servlet-name>
	<url-pattern>/VacationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>ActivityServlet</servlet-name>
	<url-pattern>/ActivityServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>AnalysisServlet</servlet-name>
	<url-pattern>/AnalysisServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>LandingServlet</servlet-name>
	<url-pattern>/LandingServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>LoginServlet</servlet-name>
	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
</web-app>